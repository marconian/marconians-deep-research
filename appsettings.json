{
  "AzureOpenAI": {
    "ReasoningEffortLevel": "Medium"
  },
  "Orchestrator": {
    "MaxResearchPasses": 3,
    "MaxFollowupQuestions": 3,
    "MaxSectionEvidenceCharacters": 2400,
    "MaxReportRevisionPasses": 2
  },
  "Researcher": {
    "MaxSearchIterations": 6,
    "MaxContinuationQueries": 2,
    "RelatedMemoryTake": 3,
    "Parallelism": {
      "NavigatorDegreeOfParallelism": 8,
      "ScraperDegreeOfParallelism": 12,
      "FileReaderDegreeOfParallelism": 4
    }
  },
  "ShortTermMemory": {
    "MaxEntries": 40,
    "SummaryBatchSize": 6,
    "CacheTtlHours": 6
  },
  "ComputerUse": {
    "Headless": false,
    "MaxConcurrentSessions": 8,
    "UsePersistentContext": true,
    "UserDataDirectory": "debug/cache/computer-use-profile",
    "PreferredLocale": "en-US",
    "Viewport": {
      "Width": 1366,
      "Height": 768,
      "DeviceScaleFactor": 1
    },
    "Stealth": {
      "Provider": "Native",
      "EnableScriptPatches": true,
      "EnableConsoleQuiets": true,
      "LogDiagnostics": false
    },
    "Proxy": {
      "Enabled": false,
      "RotatePerSession": true,
      "Endpoints": []
    },
    "Behavior": {
      "EnableMouseSimulation": true,
      "EnableTypingSimulation": true,
      "MaxMouseStepPixels": 40,
      "MinMouseDelayMs": 12,
      "MaxMouseDelayMs": 28,
      "MinClickHoldMs": 110,
      "MaxClickHoldMs": 220,
      "MinTypingDelayMs": 55,
      "MaxTypingDelayMs": 240,
      "TypingBurstCharacters": 5,
      "TypingBurstPauseMultiplier": 2.6
    },
    "Driver": {
      "ValidateRuntimeEnablePatch": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Marconian.ResearchAgent": "Debug"
    }
  }
}
